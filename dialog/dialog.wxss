/* dialog/dialog.wxss */
.dia-cover{position:fixed;left:0;top:0;bottom:0;right:0;background:rgba(0,0,0,.7);z-index:10;}
.dia-con-box{width:570rpx;height:380rpx;background:#fff;position:absolute;left:50%;top:50%;margin-left:-285rpx;margin-top:-190rpx;border-radius:5px;z-index:11;}
.dia-title{font-size:36rpx;color:#ff4747;height:88rpx;line-height:88rpx;text-align:center;border-bottom:1px solid #e6e6e6;}
.dia-foot view{height:80rpx;line-height:80rpx;font-size:32rpx;border:none;border-radius:0;text-align:center;}
.dia-foot .cancel{background:#f0f0f0;color:#999;border-bottom-left-radius:10rpx;}
.dia-foot .sure{background:#ff4747;color:#fff;border-bottom-right-radius:10rpx;}
.dia-mid{height:226rpx;padding:0 60rpx;}
.dia-mid input{height:76rpx;line-height:76rpx;background:#f0f0f0;width:100%;padding:0 30rpx;font-size:32rpx;color:#333;}
input:-webkit-input-placeholder{color:#ccc;}
/*选择性别*/
.pop-btn-close{width:56rpx;height:56rpx;position:absolute;top:18rpx;right:20rpx;}
.pop-btn-close::before,.pop-btn-close::after{content:'';position:absolute;width:6rpx;top:50%;left:50%;height:44rpx;margin-top:-22rpx;border-radius:4rpx;background:#999;margin-left:-4rpx;}
.pop-btn-close::before{transform:rotate(45deg);}
.pop-btn-close::after{transform:rotate(-45deg);}
.dia-cen{padding:60rpx 60rpx 0;text-align:center}
.circle-box{width:104rpx;height:104rpx;line-height:104rpx;text-align:center;border-radius:50%;background:#55b1ff;margin:0 auto 25rpx;}
.circle-box image{width:55rpx;height:54rpx;margin-top:25rpx;}
.women-box{color:#ff556f;}
.women-box .circle-box{background:#ff556f;}
.women-box image{width:53rpx;}
.dia-cen text{color:#55b1ff;font-size:28rpx;}
.women-box text{color:#ff556f}
radio{display:none;}
.pop-zhezhao{width:100%;height:100%;position:fixed;top:0;left:0;z-index:1000;background:#000;opacity:0.8;overflow:hidden;}
.kefu-pop-box{width:580rpx;position:fixed;top:50%;left:50%;margin:-417rpx 0 0 -290rpx;z-index:1001;background:#fff;border-radius:20rpx;}
.pop-btn-close{width:56rpx;height:56rpx;position:absolute;top:30rpx;right:30rpx;}
.pop-btn-close::before,.pop-btn-close::after{content:'';position:absolute;width:6rpx;top:50%;left:50%;height:44rpx;margin-top:-11px;border-radius:4rpx;background:#fff;margin-left:-2px;}
.pop-btn-close::before{transform:rotate(45deg);}
.pop-btn-close::after{transform:rotate(-45deg);}
.kefu-top{width:100%;height:192rpx;background:#80d3d9;padding:48rpx 0 30rpx;border-radius:20rpx 20rpx 0 0;}
.kefu-top image{width:338rpx;height:190rpx;margin:0 auto;display:block;}
.kefu-bot{width:100%;height:450rpx;}
.kefu-bot .tit{font-size:40rpx;color:#000;display:block;margin:40rpx auto 60rpx;text-align:center}
.kefu-bot .con-txt{width:430rpx;margin:0 auto 28rpx;font-size:36rpx;color:#000;align-items:center;}
.kefu-bot .con-txt text{font-size:40rpx;margin-right:20rpx;color:#666}
.kefu-bot .con-txt .btn{width:110rpx;height:56rpx;line-height:56rpx;text-align:center;margin-left:20rpx;background:#55b5ff;border-radius:10rpx;font-size:28rpx;color:#fff;}
.kefu-bot .con-txt .call{background:#ff4747;}
.kefu-bot .con-txt .box-flex{overflow:hidden}